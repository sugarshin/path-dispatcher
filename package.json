{
  "name": "path-dispatcher",
  "version": "1.1.2",
  "description": "Static url dispatcher",
  "repository": {
    "type": "git",
    "url": "git@github.com:sugarshin/path-dispatcher.git"
  },
  "main": "index.js",
  "keywords": [
    "pathname",
    "path-dispatcher",
    "dispatcher",
    "dispatch",
    "router",
    "route",
    "routing",
    "url"
  ],
  "files": [
    "index.js",
    "lib"
  ],
  "scripts": {
    "eslint": "eslint src",
    "test": "mocha --compilers js:espower-babel/guess test.js",
    "rm:lib": "rimraf lib",
    "babel": "babel -d lib --presets es2015 --no-babelrc src",
    "build": "npm run eslint && npm run rm:lib && npm run babel && npm test"
  },
  "author": "sugarshin",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-eslint": "^4.1.8",
    "babel-preset-es2015": "^6.5.0",
    "eslint": "^1.10.3",
    "eslint-config-sugarshin": "0.0.13",
    "eslint-plugin-babel": "^3.1.0",
    "espower-babel": "^4.0.1",
    "mocha": "^2.4.5",
    "power-assert": "^1.2.0",
    "rimraf": "^2.5.2"
  },
  "dependencies": {
    "glob-to-regexp": "^0.1.0"
  }
}
